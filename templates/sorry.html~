<script type="text/javascript">
    $(document).bind('loginResponse',function() {
    	var logoutUrl = '/logout?referrer=http://www.newsletter.co.uk/login?referrer=http://www.newsletter.co.uk/';
     	var screenName = loginStatusResponse.loginStatus.screenname;
     	
     	$('.UGC_userSml').html('Hello ' + screenName + '<br /><a href="' + logoutUrl 
     		+ '" class="cfo_L" title="not ' + screenName + ' ?">not ' + screenName + ' ?</a>');
     	
     	if (loginStatusResponse.loginStatus.loggedIn) {			
			$('#ugcUploadPanel').removeClass("hidden");
			$('#UGCLogin').addClass("hidden");
		} else {
			$('#ugcUploadPanel').addClass("hidden");
			$('#UGCLogin').removeClass("hidden");
		}
		
					if (loginStatusResponse.loginStatus.loggedIn) {			
				$('.ugcGenericOpenLink').removeClass("ugc_notLoggedIn").addClass("genericIn");
			} else {
				$('.ugcGenericOpenLink').removeClass("genericIn").addClass("ugc_notLoggedIn");
			}
		     	
	    $("#ugcPanel").UGC({
	    	widgetType:"generic", 
	    	maxPhoto: 3,
	    	maxVideo: 1,
	    	maxPhotoSize: 10485760,
	    	maxVideoSize: 52428800
	    });
	});
</script>																											
